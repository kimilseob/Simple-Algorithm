package _24;

import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int arr[] = new int[8];
		int avg = 0;
		int cnt = 0;
		int total  = 0;
		for(int i = 0; i < arr.length;i++) {
			arr[i] = scan.nextInt();
			
		}
		
		for(int  i = 0; i<arr.length;i++) {
			avg = avg + arr[i];
		}
		avg = avg/arr.length;
		
		for(int  i = 0; i<arr.length;i++) {
			if(avg < arr[i]) {
				cnt++;
			}
		}
		
		total  = (double)cnt / arr.length;
		
		System.out.println(total;);
		
	}
}

package _11;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		int n = 5;
		
		int arr[][] = new int[n][n];
		
		/*
		 * n=3
		 * 1 2 3
		 * 6 5 4
		 * 7 8 9
		 * 
		 */
		
		for(int i = 0;i < n ;i++) {
			for(int j = 0 ; j < n ;j++) {
				System.out.printf("%4d" , arr[i][j]);
			}
			System.out.println();
		}
	}
}

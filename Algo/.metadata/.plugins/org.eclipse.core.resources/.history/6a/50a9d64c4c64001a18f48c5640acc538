package _18;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		int input = scan.nextInt();
		
		for(int i =1 ; i<= input ; i++) {
			for(int k=0; k< input-i ; k++) {
				System.out.print(" ");
			}
			for(int j=0 ; j<=1; j++) {
				System.out.print("*");
			}
		}
	}
}
